{
  "functions": [
    {
      "source": "functions",
      "codebase": "default",
      "ignore": [
        "node_modules",
        ".git",
        "firebase-debug.log",
        "firebase-debug.*.log"
      ],
      "predeploy": [
        "npm --prefix \"$RESOURCE_DIR\" run lint"
      ]
    }
  ],
  "hosting": [{
    "target": "rn3-url",
    "frameworksBackend": {
      "region": "europe-west1"
    },
    "rewrites": [{
      "source": "**/**", "function": "url"
    }]
  }]
}
